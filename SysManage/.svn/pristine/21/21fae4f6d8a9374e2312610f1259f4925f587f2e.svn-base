<%@ page language="java" contentType="text/html; charset=GB18030"
	pageEncoding="GB18030"%>
<!DOCTYPE HTML>
<div>
	<input type="hidden" name="bookform.id">
	<table width="100%" border="0" cellspacing="2" cellpadding="3"
		align="center">
		<tr bgcolor="f5f5f5">
			<td width="30%">
				<div align="right">名称：</div>
			</td>
			<td width="70%"><input type="text" name="bookform.name" size="50"
				maxlength="50" class="easyui-validatebox" required="true"
				missingMessage="请输入名称"></td>
		</tr>
		<tr bgcolor="f5f5f5">
			<td>
				<div align="right">编码：</div>
			</td>
			<td><input type="text" name="bookform.code" size="50" maxlength="50"></td>
		</tr>
		<tr bgcolor="f5f5f5">
			<td>
				<div align="right">型号：</div>
			</td>
			<td><input type="text" name="bookform.model" size="50" maxlength="50"></td>
		</tr>
		<tr bgcolor="f5f5f5">
			<td>
				<div align="right">重量(克)：</div>
			</td>
			<td><input type="text" name="bookform.weight" size="50"
				class="easyui-validatebox" validType="number"></td>
		</tr>
		<tr bgcolor="f5f5f5">
			<td>
				<div align="right">基础价：</div>
			</td>
			<td><input type="text" name="bookform.baseprice" size="50"
				class="easyui-validatebox" validType="float" required="true"
				missingMessage="请输入基础价"></td>
		</tr>
		<tr bgcolor="f5f5f5">
			<td>
				<div align="right">市场价：</div>
			</td>
			<td><input type="text" name="bookform.marketprice" size="50"
				class="easyui-validatebox" validType="float" required="true"
				missingMessage="请输入市场价"></td>
		</tr>
		<tr bgcolor="f5f5f5">
			<td>
				<div align="right">销售价：</div>
			</td>
			<td><input type="text" name="bookform.sellprice" size="50"
				class="easyui-validatebox" validType="float" required="true"
				missingMessage="请输入销售价"></td>
		</tr>
		<tr bgcolor="f5f5f5">
			<td>
				<div align="right">销售数量：</div>
			</td>
			<td><input type="text" name="bookform.sellcount" size="50"
				class="easyui-validatebox" validType="number"></td>
		</tr>
		<tr bgcolor="f5f5f5" height="40px">
			<td>
				<div align="right">描述：</div>
			</td>
			<td><input type="text" name="bookform.description" size="80"
				maxlength="100"></td>
		</tr>
		<tr bgcolor="f5f5f5">
			<td colspan="2">
				<div align="center">
					<a href="javascript:void(0)" class="easyui-linkbutton"
						iconcls="icon-save" onclick="javascript:save();"> 保存</a> <a
						href="javascript:void(0)" class="easyui-linkbutton"
						iconcls="icon-cancel" onclick="javascript:cancel();"> 关闭</a>
				</div>
			</td>
		</tr>
	</table>
</div>